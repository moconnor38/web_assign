@using SampleTemplate.Models
@model IEnumerable<Systemreqs>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";}

<h2>See below for more detailed information</h2>
<body>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Title</th>
            <th>Processor</th>
            <th>Memory</th>
            <th>Storage</th>
            <th>Video</th>
        </tr>
        @foreach (Systemreqs req in Model)
        {
            <tr>
                <td>@req.Title</td>
                <td>@req.Processor</td>
                <td>@req.Memory</td>
                <td>@req.Storage</td>
                <td>@req.Video</td>
            </tr>
        }
    </table>
</body>